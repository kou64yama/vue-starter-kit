{
  "name": "web",
  "version": "0.0.0",
  "engines": {
    "node": ">=6.13.1",
    "npm": ">=3.10.10"
  },
  "dependencies": {
    "express": "^4.16.3",
    "node-fetch": "^2.2.0",
    "vue": "^2.5.17",
    "vue-router": "^3.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@types/browser-sync": "^0.0.42",
    "@types/chokidar": "^1.7.5",
    "@types/express": "^4.16.0",
    "@types/glob": "^7.1.1",
    "@types/mkdirp": "^0.5.2",
    "@types/node": "^10.11.4",
    "@types/node-fetch": "^2.1.2",
    "@types/rimraf": "^2.0.2",
    "@types/webpack": "^4.4.14",
    "@types/webpack-assets-manifest": "^3.0.0",
    "@types/webpack-bundle-analyzer": "^2.13.0",
    "@types/webpack-dev-middleware": "^2.0.2",
    "@types/webpack-env": "^1.13.6",
    "@types/webpack-hot-middleware": "^2.16.4",
    "@types/webpack-node-externals": "^1.6.3",
    "autoprefixer": "^9.1.5",
    "babel-core": "^7.0.0-bridge.0",
    "browser-sync": "^2.26.0",
    "chokidar": "^2.0.4",
    "css-loader": "^1.0.0",
    "file-loader": "^2.0.0",
    "glob": "^7.1.3",
    "handlebars": "^4.0.12",
    "handlebars-loader": "^1.7.0",
    "husky": "^1.1.1",
    "jest": "^23.6.0",
    "jest-serializer-vue": "^2.0.2",
    "lint-staged": "^7.3.0",
    "mkdirp": "^0.5.1",
    "null-loader": "^0.1.1",
    "pixrem": "^4.0.1",
    "pleeease-filters": "^4.0.0",
    "postcss": "^7.0.5",
    "postcss-calc": "^6.0.2",
    "postcss-color-function": "^4.0.1",
    "postcss-custom-media": "^7.0.4",
    "postcss-custom-properties": "^8.0.8",
    "postcss-custom-selectors": "^5.1.2",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-import": "^12.0.0",
    "postcss-loader": "^3.0.0",
    "postcss-media-minmax": "^4.0.0",
    "postcss-nested": "^4.1.0",
    "postcss-nesting": "^7.0.0",
    "postcss-selector-matches": "^4.0.0",
    "postcss-selector-not": "^4.0.0",
    "prettier": "^1.14.3",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.6.2",
    "svg-url-loader": "^2.3.2",
    "ts-jest": "^23.10.4",
    "ts-loader": "^5.2.1",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-plugin-prettier": "^2.0.0",
    "typescript": "^3.1.1",
    "url-loader": "^1.1.1",
    "vue-hot-reload-api": "^2.3.1",
    "vue-jest": "^2.6.0",
    "vue-loader": "^15.4.2",
    "vue-server-renderer": "^2.5.17",
    "vue-template-compiler": "^2.5.17",
    "webpack": "^4.20.2",
    "webpack-assets-manifest": "^3.1.0",
    "webpack-bundle-analyzer": "^3.0.2",
    "webpack-dev-middleware": "^3.4.0",
    "webpack-hot-middleware": "^2.24.2",
    "webpack-node-externals": "^1.7.2"
  },
  "babel": {
    "inputSourceMap": true
  },
  "lint-staged": {
    "ignore": [
      "package.json"
    ],
    "linters": {
      "*.{ts,tsx}": [
        "tslint --fix",
        "git add --force"
      ],
      "*.{json,md}": [
        "prettier --write",
        "git add --force"
      ]
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "scripts": {
    "lint-ts": "tslint --project .",
    "lint-css": "stylelint \"src/**/*.{css,less,styl,scss,sass,sss}\"",
    "lint": "yarn run lint-ts && yarn run lint-css",
    "fix-ts": "yarn run lint-ts --fix",
    "fix-css": "yarn run lint-css --fix",
    "fix": "yarn run fix-ts && yarn run fix-css",
    "test": "jest",
    "test-watch": "yarn run test --watch --notify",
    "test-cover": "yarn run test --coverage",
    "coverage": "yarn run test-cover && opn coverage/lcov-report/index.html",
    "clean": "ts-node tools/run clean",
    "copy": "ts-node tools/run copy",
    "bundle": "ts-node tools/run bundle",
    "build": "ts-node tools/run build",
    "build-stats": "yarn run build --release --analyse",
    "render": "ts-node tools/run render",
    "serve": "ts-node tools/run runServer",
    "start": "ts-node tools/run start"
  }
}
