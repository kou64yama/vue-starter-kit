  "babel": {
    "inputSourceMap": true
  },
  "lint-staged": {
    "ignore": [
      "package.json"
    ],
    "linters": {
      "*.{ts,tsx}": [
        "eslint --fix",
        "git add --force"
      ],
      "*.{json,md}": [
        "prettier --write",
        "git add --force"
      ]
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "scripts": {
    "lint-ts": "eslint --ignore-path .gitignore --ignore-pattern \"!**/.*\" --ignore-pattern build .",
    "lint-css": "stylelint \"src/**/*.{css,less,styl,scss,sass,sss}\"",
    "lint": "yarn run lint-ts && yarn run lint-css",
    "fix-ts": "yarn run lint-ts --fix",
    "fix-css": "yarn run lint-css --fix",
    "fix": "yarn run fix-ts && yarn run fix-css",
    "test": "jest",
    "test-watch": "yarn run test --watch --notify",
    "test-cover": "yarn run test --coverage",
    "coverage": "yarn run test-cover && opn coverage/lcov-report/index.html",
    "clean": "ts-node -T tools/run clean",
    "copy": "ts-node -T tools/run copy",
    "bundle": "ts-node -T tools/run bundle",
    "build": "ts-node -T tools/run build",
    "build-stats": "yarn run build --release --analyse",
    "deploy": "ts-node -T tools/run deploy",
    "render": "ts-node -T tools/run render",
    "serve": "ts-node -T tools/run runServer",
    "start": "ts-node -T tools/run start"
  }
}
